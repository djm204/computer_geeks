<h1>Listing watcheditems</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @watcheditems.each do |watcheditem| %>
      <tr>
        <td><%= link_to 'Show', watcheditem %></td>
        <td><%= link_to 'Edit', edit_watcheditem_path(watcheditem) %></td>
        <td><%= link_to 'Destroy', watcheditem, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Watcheditem', new_watcheditem_path %>
