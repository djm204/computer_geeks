%h2= "Search Results for '#{ params[:search_keywords]}'" 
- if @found_products.empty?
  %h3 No products found!
- else
  %section.search
    = paginate @found_products
  = render partial: 'shared/product',
           collection: @found_products, locals:{partial_name:'shared/buttons', button_name: 'Add to Cart'}
  - if @found_products.count > 2
    = paginate @found_products